<!DOCTYPE html>
<html lang="en">
<head>
    <title>EJS View Test</title>
</head>
<body>
    <h1><%= title %></h1>

    <h2><%= topic %></h2>

    <!-- <h3><%= status %></h3> -->

    <h3>
        <%= status %>
        <% if(status==0) { %>
        Not started
        <% }else if (status==1) { %>
            Going on
        <% } else { %>
            Done
        <% } %>
        
    </h3>

    <% fruits.forEach(function(fruit){ %>
        <h4> <%= fruit %> <br></h4>
    <% }) %>

    <h5>
        <%= error.code %> <%=error.msg%>
    </h5>

</body>
</html>
